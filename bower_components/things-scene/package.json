{
  "name": "things-scene",
  "version": "0.5.2",
  "description": "2D graphic library",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heartyoh/things-scene.git"
  },
  "keywords": [
    "2D",
    "graphic",
    "library",
    "canvas"
  ],
  "scripts": {
    "compile": "./node_modules/.bin/babel --presets es2015 -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "./node_modules/.bin/mocha --require should --compilers js:babel-core/register test/**/test-*.js",
    "pretest": "npm run compile",
    "watchify": "watchify --extension=js -o things-scene.js index.js",
    "start": "watchify --extension=js -o things-scene.js index.js & python -m SimpleHTTPServer",
    "build": "browserify index.js -o things-scene.js && uglifyjs things-scene.js -c -o things-scene-min.js"
  },
  "author": "heartyoh@hatiolab.com",
  "license": "ISC",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "~7.2.0",
    "browserify": "~10.2.4",
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "should": "^8.0.2",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.3.1"
  },
  "bugs": {
    "url": "https://github.com/heartyoh/things-scene/issues"
  },
  "homepage": "https://github.com/heartyoh/things-scene#readme",
  "dependencies": {
    "bwip": "^0.7.1"
  }
}
